<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="amalfi.wgu_c482.ModProductController">
   <children>
      <HBox prefHeight="502.0" prefWidth="798.0" spacing="20.0">
         <children>
            <VBox prefWidth="400.0">
               <children>
                  <StackPane prefHeight="70.0" prefWidth="379.0">
                     <children>
                        <Label text="Modify Product" StackPane.alignment="CENTER_LEFT">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font></Label>
                     </children>
                  </StackPane>
                  <GridPane prefHeight="180.0" prefWidth="379.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="120.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="80.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="120.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="80.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Label text="ID" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2">
                           <children>
                              <TextField disable="true" prefHeight="30.0" prefWidth="100.0" promptText="Auto Gen - Disabled" StackPane.alignment="CENTER_LEFT">
                                 <font>
                                    <Font size="12.0" />
                                 </font></TextField>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="1">
                           <children>
                              <Label text="Name" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1">
                           <children>
                              <TextField fx:id="prodNameField" maxHeight="-Infinity" prefHeight="30.0" prefWidth="158.0">
                                 <font>
                                    <Font size="12.0" />
                                 </font></TextField>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="2">
                           <children>
                              <Label text="Stock" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                           <children>
                              <TextField fx:id="prodStockField" maxHeight="-Infinity" prefHeight="30.0" prefWidth="100.0" StackPane.alignment="CENTER_LEFT">
                                 <font>
                                    <Font size="12.0" />
                                 </font></TextField>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="3">
                           <children>
                              <Label text="Price" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                           <children>
                              <TextField fx:id="prodPriceField" prefHeight="30.0" prefWidth="100.0" StackPane.alignment="CENTER_LEFT">
                                 <font>
                                    <Font size="12.0" />
                                 </font></TextField>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="4">
                           <children>
                              <Label text="Max Stock" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <children>
                              <TextField fx:id="prodMaxField" maxHeight="-Infinity" prefHeight="30.0" prefWidth="100.0" StackPane.alignment="CENTER_LEFT">
                                 <font>
                                    <Font size="12.0" />
                                 </font></TextField>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
                           <children>
                              <Label text="Min Stock" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
                           <children>
                              <TextField fx:id="prodMinField" maxHeight="-Infinity" prefHeight="30.0">
                                 <font>
                                    <Font size="12.0" />
                                 </font></TextField>
                           </children>
                        </StackPane>
                     </children>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </GridPane>
                  <VBox prefHeight="92.0" prefWidth="379.0">
                     <children>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Label fx:id="exceptMinMax" text="Exception: Min Stock must be less than Max Stock" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Label fx:id="exceptProdName" text="Exception: Product must have a Name" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Label fx:id="exceptPriceDouble" text="Exception: Price must be a double (e.g., 8.99)" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Label fx:id="exceptMaxStockInt" text="Exception: Max Stock must be an integer" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Label fx:id="exceptStockInt" text="Exception: Min Stock must be an integer" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                           </children>
                        </StackPane>
                     </children>
                     <padding>
                        <Insets top="50.0" />
                     </padding></VBox>
               </children></VBox>
            <VBox prefWidth="400.0">
               <children>
                  <StackPane prefHeight="75.0" prefWidth="379.0">
                     <children>
                        <TextField fx:id="searchParts" maxWidth="-Infinity" prefWidth="200.0" promptText="Search by Part ID or Name" StackPane.alignment="CENTER_RIGHT">
                           <font>
                              <Font size="12.0" />
                           </font></TextField>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <TableView prefHeight="200.0" prefWidth="400.0">
                          <columns>
                            <TableColumn fx:id="partIdSearchCol" prefWidth="90.0" text="Part ID" />
                            <TableColumn fx:id="partNameSearchCol" prefWidth="90.0" text="Part Name" />
                              <TableColumn fx:id="partStockSearchCol" prefWidth="90.0" text="In Stock" />
                              <TableColumn fx:id="partPriceSearchCol" prefWidth="90.0" text="Price" />
                          </columns>
                        </TableView>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="95.0" prefWidth="379.0">
                     <children>
                        <Button fx:id="AddToProd" mnemonicParsing="false" onAction="#onAddToProd" text="Add Part to Product" StackPane.alignment="CENTER_RIGHT">
                           <font>
                              <Font size="12.0" />
                           </font></Button>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <TableView prefHeight="200.0" prefWidth="400.0">
                          <columns>
                            <TableColumn fx:id="partIdAssocCol" prefWidth="90.0" text="Part ID" />
                            <TableColumn fx:id="partNameAssocCol" prefWidth="90.0" text="Part Name" />
                              <TableColumn fx:id="partStockAssocCol" prefWidth="90.0" text="In Stock" />
                              <TableColumn fx:id="partPriceAssocCol" prefWidth="90.0" text="Price" />
                          </columns>
                        </TableView>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="90.0" prefWidth="379.0">
                     <children>
                        <Button fx:id="removeAssocPart" mnemonicParsing="false" onAction="#onRemoveAssocPart" text="Remove Associated Part" StackPane.alignment="CENTER_RIGHT">
                           <font>
                              <Font size="12.0" />
                           </font></Button>
                     </children>
                  </StackPane>
                  <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="50.0" prefWidth="379.0">
                     <children>
                        <StackPane prefHeight="32.0" prefWidth="72.0">
                           <children>
                              <Button fx:id="modProdSave" mnemonicParsing="false" onAction="#onModProdSave" text="Save" StackPane.alignment="CENTER_RIGHT">
                                 <font>
                                    <Font size="12.0" />
                                 </font></Button>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="32.0" prefWidth="86.0">
                           <children>
                              <Button fx:id="modProdCancel" mnemonicParsing="false" onAction="#onModProdCancel" text="Cancel" StackPane.alignment="CENTER_RIGHT">
                                 <font>
                                    <Font size="12.0" />
                                 </font></Button>
                           </children>
                        </StackPane>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></HBox>
   </children>
</AnchorPane>
