<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="amalfi.wgu_c482.ModPartController">
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
   </padding>
   <VBox prefHeight="600.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <HBox prefHeight="100.0" prefWidth="600.0">
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label text="Add Part"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <RadioButton fx:id="modInHouseRadio" mnemonicParsing="false" onAction="#onModInHouseRadio"
                         text="In-House"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <RadioButton fx:id="modOutSourcedRadio" mnemonicParsing="false" onAction="#onModOutsourcedRadio"
                         text="Outsourced"/>
         </StackPane>
      </HBox>
      <GridPane prefHeight="266.0" prefWidth="600.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
         </rowConstraints>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label text="ID" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="1">
            <Label text="Name" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="2">
            <Label text="Stock" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="3">
            <Label text="Price" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="4">
            <Label text="Max Stock" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="5">
            <Label fx:id="machineIdLabel" text="Machine ID" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1">
            <TextField disable="true" promptText="Auto Gen - Disabled">
               <font>
                  <Font size="12.0"/>
               </font>
            </TextField>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <TextField fx:id="partNameField"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <TextField fx:id="partStockField"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <TextField fx:id="partPriceField"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
            <TextField fx:id="partMaxField"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
            <TextField fx:id="machineField"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
            <Label text="Min Stock"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
            <TextField fx:id="partMinField" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="6">
            <Label fx:id="companyNameLabel" text="Company Name" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
            <TextField fx:id="companyField"/>
         </StackPane>
      </GridPane>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="560.0">
         <StackPane prefHeight="100.0" prefWidth="110.0">
            <Button fx:id="AddPartSave" mnemonicParsing="false" onAction="#onModPartSave" text="Save"
                    StackPane.alignment="CENTER"/>
         </StackPane>
         <StackPane prefHeight="100.0" prefWidth="211.0">
            <Button fx:id="addPartCancel" mnemonicParsing="false" onAction="#onModPartCancel" text="Cancel"
                    StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
      </HBox>
      <VBox prefHeight="150.0" prefWidth="100.0">
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptCompName" text="Outsourced Parts must have Company Name" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptBetweenMinMax" text="Exception: Stock must be between Min Stock and Max Stock"
                   textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptMinMax" text="Exception: Min Stock must be less than Max Stock" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptPartName" text="Exception: Part must have a Name" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptStockInt" text="Exception: Stock must be an integer" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptPriceDouble" text="Exception: Price must be a double (e.g., 8.99)"
                   textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptMaxStockInt" text="Exception: Max Stock must be an integer" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptMachineInt" text="Exception: Machine ID must be an integer" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
         <StackPane prefHeight="150.0" prefWidth="200.0">
            <Label fx:id="exceptMinStockInt" text="Exception: Min Stock must be an integer" textFill="RED"
                   wrapText="true" StackPane.alignment="CENTER_LEFT"/>
         </StackPane>
      </VBox>
   </VBox>
</AnchorPane>
