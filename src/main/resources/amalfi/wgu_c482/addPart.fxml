<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="amalfi.wgu_c482.AddPartController">
   <children>
      <VBox prefHeight="600.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="600.0">
               <children>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label text="Add Part" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <RadioButton fx:id="inHouseRadio" mnemonicParsing="false" onAction="#onAddInHouseRadio" text="In-House" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <RadioButton fx:id="addOutSourcedRadio" mnemonicParsing="false" onAction="#onAddOutsourcedRadio" text="Outsourced" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <GridPane prefHeight="266.0" prefWidth="600.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label text="ID" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="1">
                     <children>
                        <Label text="Name" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="2">
                     <children>
                        <Label text="Current Stock" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="3">
                     <children>
                        <Label text="Price" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="4">
                     <children>
                        <Label text="Max Stock" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="5">
                     <children>
                        <Label fx:id="machineIdLabel" text="Machine ID" StackPane.alignment="CENTER_LEFT" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1">
                     <children>
                        <TextField disable="true" promptText="Auto Gen - Disabled">
                           <font>
                              <Font size="12.0" />
                           </font></TextField>
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <children>
                        <TextField fx:id="partNameField" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <children>
                        <TextField fx:id="partStockField" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <children>
                        <TextField fx:id="partPriceField" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                     <children>
                        <TextField fx:id="partMaxField" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <children>
                        <TextField fx:id="machineField" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
                     <children>
                        <Label text="Min Stock" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
                     <children>
                        <TextField fx:id="partMinField" StackPane.alignment="CENTER_LEFT" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="6">
                     <children>
                        <Label fx:id="companyNameLabel" text="Company Name" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
                     <children>
                        <TextField fx:id="companyField" />
                     </children>
                  </StackPane>
               </children>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="560.0">
               <children>
                  <StackPane prefHeight="100.0" prefWidth="110.0">
                     <children>
                        <Button fx:id="AddPartSave" mnemonicParsing="false" onAction="#onAddPartSave" text="Save" StackPane.alignment="CENTER" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="100.0" prefWidth="211.0">
                     <children>
                        <Button fx:id="addPartCancel" mnemonicParsing="false" onAction="#onAddPartCancel" text="Cancel" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <VBox prefHeight="150.0" prefWidth="100.0">
               <children>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptMinMax" text="Exception: Min Stock must be less than Max Stock" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptPartName" text="Exception: Part must have a Name" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptStockInt" text="Exception: Current Stock must be an integer" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptPriceDouble" text="Exception: Price must be a double (e.g., 8.99)" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptMaxStockInt" text="Exception: Max Stock must be an integer" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children></StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptMachineInt" text="Exception: Machine ID must be an integer" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="exceptMinStockInt" text="Exception: Min Stock must be an integer" textFill="RED" wrapText="true" StackPane.alignment="CENTER_LEFT" />
                     </children>
                  </StackPane>
               </children>
            </VBox>
         </children></VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</AnchorPane>
